<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.trump.vincent.mybatis.mapper.UserMapper">

    <insert id="save" parameterType="org.trump.vincent.mybatis.domain.User">
        INSERT INTO TB_USER(id,`name`,`password`,age,sex)
        VALUES (#{userId},#{username},#{password},#{age},#{sex})
    </insert>
</mapper>